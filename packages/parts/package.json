{
  "name": "@hvsb/parts",
  "version": "0.0.1",
  "description": "Components for use in HVSB.app",
  "type": "module",
  "svelte": "src/lib/index.ts",
  "main": "src/lib/index.ts",
  "scripts": {
    "dev": "svelte-kit dev -p 3010",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "prepare": "svelte-kit sync",
    "check": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --threshold warning --diagnostic-sources js,svelte --watch",
    "release": "npm run package && cd package && npm publish",
    "test": "vitest -u",
    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@hvsb/types": "workspace:^0.0.1",
    "@sveltejs/adapter-auto": "1.0.0-next.52",
    "@sveltejs/kit": "1.0.0-next.354",
    "@sveltejs/vite-plugin-svelte": "1.0.0-next.49",
    "@testing-library/svelte": "^3.1.2",
    "happy-dom": "^2.49.0",
    "instantsearch.js": "^4.42.0",
    "kitbook": "^0.0.15",
    "mdsvex": "^0.10.6",
    "svelte": "^3.48.0",
    "svelte-check": "^2.7.2",
    "svelte-deep-wind-preprocess": "^0.0.7",
    "svelte-pieces": "^1.0.27",
    "svelte-preprocess": "^4.10.7",
    "svelte-store-router": "^2.0.10",
    "svelte-windicss-preprocess": "~4.1.0",
    "svelte2tsx": "^0.5.10",
    "tslib": "^2.4.0",
    "typescript": "~4.7.4",
    "vitest": "^0.15.2",
    "windicss": "^3.5.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HVSBible/hvsb.git"
  },
  "author": "HVSB",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HVSBible/hvsb/issues"
  },
  "homepage": "https://github.com/HVSBible/hvsb#readme"
}