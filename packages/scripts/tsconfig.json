{
  "compilerOptions": {
    "moduleResolution": "node",
		"module": "es2020",
		"lib": [
			"es2020",
			"DOM"
		],
		"target": "es2020",
    "strict": true,
    // "strictNullChecks": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    // "resolveJsonModule": true,
    "types": [
      "vitest/globals",
      "vitest/importMeta"
    ]
  }
}