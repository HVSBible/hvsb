{
  "name": "hvsb",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "dev": "pnpm --filter=site dev",
    "build": "pnpm --filter=site build",
    "preview": "pnpm --filter=site preview",
    "kitbook": "pnpm --filter=site kitbook",
    "kitbook:build": "pnpm --filter=site kitbook:build",
    "kitbook:preview": "pnpm --filter=site kitbook:preview",
    "test": "pnpm vitest",
    "lint": "eslint .",
    "format-check": "prettier --check --plugin-search-dir=. .",
    "format": "prettier --write --plugin-search-dir=. .",
    "check-packages": "pnpm update --interactive --recursive --latest"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "prettier": "^2.8.3",
    "prettier-plugin-svelte": "^2.9.0",
    "svelte": "^3.55.1",
    "typescript": "~4.9.5",
    "vitest": "^0.34.4"
  }
}